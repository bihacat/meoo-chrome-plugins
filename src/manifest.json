{
  "manifest_version": 2,
  "name": "Meoo",
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "version": "2.0.0",
  "description": "Meoo工具集合",
  "background": {
    "scripts": [
      "runtime/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://mooc1.chaoxing.com/mycourse/studentstudy?*",
        "https://mooc1-1.chaoxing.com/mycourse/studentstudy?*",
        "https://mooc1-2.chaoxing.com/mycourse/studentstudy?*",
        "https://mooc1-3.chaoxing.com/mycourse/studentstudy?*"
      ],
      "js": [
        "runtime/jquery.min.js",
        "chaoxing/main.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://www.mosoteach.cn/web/index.php?*"
      ],
      "js": [
        "runtime/jquery.min.js",
        "mosoteach/main.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://nfstar.net/*"
      ],
      "js": [
        "runtime/jquery.min.js",
        "nfstar/main.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://www.icourse163.org/*"
      ],
      "js": [
        "runtime/jquery.min.js",
        "icourse163/main.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://m.rr.tv/detail/*"
      ],
      "js": [
        "runtime/jquery.min.js",
        "mrrtv/main.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://m.rr.tv/detail/*",
    "https://www.mosoteach.cn/web/index.php?*"
  ],
  "page_action": {
    "default_popup": "runtime/popup.html",
    "default_icon": "assets/icon.png"
  }
}
